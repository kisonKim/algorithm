   "source": [
    "N,K = map(int, input().split())\n",
    "#1빼거나, K로 나누거나 선택\n",
    "#처음에 K로 나눠지는지 확인\n",
    "#나눠지면 K로 나눠봄\n",
    "#나눠지지 않으면 1뺌\n",
    "count = 0\n",
    "while True :\n",
    "    if N == 1 :\n",
    "        break\n",
    "    if N%K == 0 : \n",
    "        N /= K\n",
    "        count+=1\n",
    "    else :\n",
    "        N -= 1\n",
    "        count+=1\n",
    "\n",
    "print(count)"
   ]
  },
